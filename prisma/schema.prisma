generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String   @id @default(cuid())
  userId          String
  experienceLevel String?
  favoriteSystems String[]
  gameStyle       String[]
  safety          String[]
  goals           String[]
  logistics       String[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

